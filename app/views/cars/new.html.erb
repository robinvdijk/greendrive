<div id="pagewrap">
	<div class="wrapper fullwidth">
		<%= form_for(@car) do |f| %>
		
			<ul>
				<li>
					<h2>Auto koppelen</h2>
				</li>
				<li>
					<%= f.label :username, 'Username Sycada' %><br />
					<%= f.text_field :username, :class => "field" %>
				</li>
				<li>	
					<%= f.label :password, 'Wachtwoord Sycada' %><br />
					<%= f.password_field :password, :class => "field" %>
				</li>
				<li>
					<%= f.label :license_plate, 'Kenteken' %><br />
					<%= f.text_field :license_plate, :class => "field" %>
				</li>
				<li>
					<%= f.label :model, 'Model' %><br />
					<%= f.text_field :model, :class => "field" %>
				</li>
				<li>
					<%= f.submit 'Koppelen', :class => "button" %>
				</li>
			</ul>
			
		<% end %>
					
		
	</div>
</div>
