
<div id="progressbar"></div>

	<div class="header">
		
		<% if controller_name == "sessions"  || controller_name == "password_resets" || controller_name == "pages" %>
			<div id="pagewrap">
				<ul class="nav">
					<li class="home">
						<%= link_to image_tag("header_logo_white.png"), root_path %>
					</li>
				</ul>
			</div>
			
		<% else %>
		
		

		<div id="pagewrap">

			<ul class="nav">
				<li class="home">
					<%= link_to image_tag("header_logo_white.png"), root_path %>
				</li>
				<li>
					<%= link_to image_tag("help.svg"), new_car_path %>
				</li>				
				<li>
					<%= image_tag("menu.png", :alt => "menu") %>
				       <% if current_user %>
					    <ul>
							
				            <li><%= link_to "Mijn profiel", user_path(current_user) %></li>

				            <li><%= link_to "Uitloggen", logout_path, :method => :delete %></li>
							
				        </ul>
						<% end %>
				</li>
				
				<li>
					<input type=search results=5 autosave=some_unique_value autofocus name=s>
				</li>
				
			</ul>

		
		</div>
		<% end %>
		
		
		
	
	<!-- <%= link_to "Gebruikers", users_path %> -->

	</div>



