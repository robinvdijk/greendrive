<%= form_for(resource, :as => resource_name, :url => edit_user_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>
  
  <div><%= f.label :first_name, "Firstname" %><br />
  <%= f.text_field :first_name %></div>
  
  <div><%= f.label :last_name, "Lastname" %><br />
	  <%= f.text_field :last_name %>
  </div>
  
  <div> <%= f.label :user_name, "Username" %><br />
	  <%= f.text_field :user_name %>
  </div>
  
  <div><%= f.label :license_plate, "License Plate" %><br />
	  <%= f.text_field :license_plate %>
  </div>
<% end %>